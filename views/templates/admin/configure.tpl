<form method="POST" class="defaultForm form-horizontal">
    <div class="panel">
        <div class="panel-heading">
            {l s='Parameters' mod='calculator'}
        </div>
        <div class="form-group">
            <div class="col-lg-12">
                <label class="control-label col-lg-4">{l s='a = partner' mod='calculator'}</label>
                <div class="col-lg-6">
                    <input type="text" name="a" class="form-control" value="{$CALCULATOR_A}" />
                </div>
            </div>
            <div class="col-lg-12">
                <label class="control-label col-lg-4">{l s='b = minimun amount (50)' mod='calculator'}</label>
                <div class="col-lg-6">
                    <input type="text" name="b" class="form-control" value="{$CALCULATOR_B}" />
                </div>
            </div>
            <div class="col-lg-12">
                <label class="control-label col-lg-4">{l s='c = maximum amount (5000)' mod='calculator'}</label>
                <div class="col-lg-6">
                    <input type="text" name="c" class="form-control" value="{$CALCULATOR_C}" />
                </div>
            </div>
            <div class="col-lg-12">
                <label class="control-label col-lg-4">{l s='d = default amount (2000)' mod='calculator'}</label>
                <div class="col-lg-6">
                    <input type="text" name="d" class="form-control" value="{$CALCULATOR_D}" />
                </div>
            </div>
            <div class="col-lg-12">
                <label class="control-label col-lg-4">{l s='e = minimum period (3)' mod='calculator'}</label>
                <div class="col-lg-6">
                    <input type="text" name="e" class="form-control" value="{$CALCULATOR_E}" />
                </div>
            </div>
            <div class="col-lg-12">
                <label class="control-label col-lg-4">{l s='f = maximum period (48)' mod='calculator'}</label>
                <div class="col-lg-6">
                    <input type="text" name="f" class="form-control" value="{$CALCULATOR_F}" />
                </div>
            </div>
            <div class="col-lg-12">
                <label class="control-label col-lg-4">{l s='g = default period (24)' mod='calculator'}</label>
                <div class="col-lg-6">
                    <input type="text" name="g" class="form-control" value="{$CALCULATOR_G}" />
                </div>
            </div>
            <div class="col-lg-12">
                <label class="control-label col-lg-4">{l s='h = step of period (3)' mod='calculator'}</label>
                <div class="col-lg-6">
                    <input type="text" name="h" class="form-control" value="{$CALCULATOR_H}" />
                </div>
            </div>
            <div class="col-lg-12">
                <label class="control-label col-lg-4">{l s='i = minimum downpayment (0)' mod='calculator'}</label>
                <div class="col-lg-6">
                    <input type="text" name="i" class="form-control" value="{$CALCULATOR_I}" />
                </div>
            </div>
            <div class="col-lg-12">
                <label class="control-label col-lg-4">{l s='j = maximum downpayment (90)' mod='calculator'}</label>
                <div class="col-lg-6">
                    <input type="text" name="j" class="form-control" value="{$CALCULATOR_J}" />
                </div>
            </div>
            <div class="col-lg-12">
                <label class="control-label col-lg-4">{l s='k = default downpayment (10)' mod='calculator'}</label>
                <div class="col-lg-6">
                    <input type="text" name="k" class="form-control" value="{$CALCULATOR_K}" />
                </div>
            </div>
            <div class="col-lg-12">
                <label class="control-label col-lg-4">{l s='l = step of downpayment (10)' mod='calculator'}</label>
                <div class="col-lg-6">
                    <input type="text" name="l" class="form-control" value="{$CALCULATOR_L}" />
                </div>
            </div>
            <div class="col-lg-12">
                <label class="control-label col-lg-4">{l s='m = interest (20.9)' mod='calculator'}</label>
                <div class="col-lg-6">
                    <input type="text" name="m" class="form-control" value="{$CALCULATOR_M}" />
                </div>
            </div>
            <div class="col-lg-12">
                <label class="control-label col-lg-4">{l s='n = contract fee (5.5) ' mod='calculator'}</label>
                <div class="col-lg-6">
                    <input type="text" name="n" class="form-control" value="{$CALCULATOR_N}" />
                </div>
            </div>
            <div class="col-lg-12">
                <label class="control-label col-lg-4">{l s='o = contract fee min (10)' mod='calculator'}</label>
                <div class="col-lg-6">
                    <input type="text" name="o" class="form-control" value="{$CALCULATOR_O}" />
                </div>
            </div>
            <div class="col-lg-12">
                <label class="control-label col-lg-4">{l s='p = admin fee (0)' mod='calculator'}</label>
                <div class="col-lg-6">
                    <input type="text" name="p" class="form-control" value="{$CALCULATOR_P}" />
                </div>
            </div>
            <div class="col-lg-12">
                <label
                    class="control-label col-lg-4">{l s='q = zero downpayment maximum amount (1400)' mod='calculator'}</label>
                <div class="col-lg-6">
                    <input type="text" name="q" class="form-control" value="{$CALCULATOR_Q}" />
                </div>
            </div>
            <div class="col-lg-12">
                <label class="control-label col-lg-4">{l s='r = disclaimer text' mod='calculator'}</label>
                <div class="col-lg-6">
                    <input type="text" name="r" class="form-control" value="{$CALCULATOR_R}" />
                </div>
            </div>
            <div class="col-lg-12">
                <label class="control-label col-lg-4">{l s='s = template (white)' mod='calculator'}</label>
                <div class="col-lg-6">
                    <input type="text" name="s" class="form-control" value="{$CALCULATOR_S}" />
                </div>
            </div>
            <div class="col-lg-12">
                <label
                    class="control-label col-lg-4">{l s='t = complicated predefined disclaimer text' mod='calculator'}</label>
                <div class="col-lg-6">
                    <input type="text" name="t" class="form-control" value="{$CALCULATOR_T}" />
                </div>
            </div>
            <div class="col-lg-12">
                <label
                    class="control-label col-lg-4">{l s='t-amount = disclaimer loan amount (-)' mod='calculator'}</label>
                <div class="col-lg-6">
                    <input type="text" name="t-amount" class="form-control" value="{$CALCULATOR_T_AMOUNT}" />
                </div>
            </div>
            <div class="col-lg-12">
                <label class="control-label col-lg-4">{l s='t-period = disclaimer period (-)' mod='calculator'}</label>
                <div class="col-lg-6">
                    <input type="text" name="t-period" class="form-control" value="{$CALCULATOR_T_PERIOD}" />
                </div>
            </div>
            <div class="col-lg-12">
                <label
                    class="control-label col-lg-4">{l s='t-interest = disclaimer interest rate (-)' mod='calculator'}</label>
                <div class="col-lg-6">
                    <input type="text" name="t-interest" class="form-control" value="{$CALCULATOR_T_INTEREST}" />
                </div>
            </div>
            <div class="col-lg-12">
                <label
                    class="control-label col-lg-4">{l s='t-commision = disclaimer commision fee (-)' mod='calculator'}</label>
                <div class="col-lg-6">
                    <input type="text" name="t-commision" class="form-control" value="{$CALCULATOR_T_COMMISION}" />
                </div>
            </div>
            <div class="col-lg-12">
                <label
                    class="control-label col-lg-4">{l s='t-adminfee = discalimer admin fee (0)' mod='calculator'}</label>
                <div class="col-lg-6">
                    <input type="text" name="t-adminfee" class="form-control" value="{$CALCULATOR_T_ADMINFEE}" />
                </div>
            </div>
            <div class="col-lg-12">
                <label
                    class="control-label col-lg-4">{l s='t-total = disclaimer total amount to repaid (-)' mod='calculator'}</label>
                <div class="col-lg-6">
                    <input type="text" name="t-total" class="form-control" value="{$CALCULATOR_T_TOTAL}" />
                </div>
            </div>
            <div class="col-lg-12">
                <label class="control-label col-lg-4">{l s='t-apr = disclaimer apr (-)' mod='calculator'}</label>
                <div class="col-lg-6">
                    <input type="text" name="t-apr" class="form-control" value="{$CALCULATOR_T_APR}" />
                </div>
            </div>
        </div>
        <input type="submit" name="submitUpdate" value="Save" class="btn btn-default">
    </div>
</form>